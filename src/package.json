    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^4.0.1",
    "koa-helmet": "^5.2.0",
    "koa-jsonp": "^2.0.2",
    "koa-jwt": "^4.0.0",
    "koa-router": "^9.0.1",
    "koa-session": "^6.0.0",
    "koa-socket": "^4.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
