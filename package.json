{
  "name": "koa2",
  "version": "1.0.0",
  "description": "koa server!",
  "main": "app.js",
  "scripts": {
    "dev": "export NODE_ENV='development' && supervisor app.js",
    "serve": "export NODE_ENV='production' && node app.js",
    "start": "export NODE_ENV='production' && pm2 start app.js --name www_koa2_com",
    "restart": "pm2 restart www_koa2_com",
    "stop": "pm2 stop www_koa2_com",
    "del": "pm2 delete www_koa2_com"
  },
  "author": "ManKeung",
  "license": "MIT",
  "dependencies": {
    "@koa/multer": "^2.0.1",
    "art-template": "^4.13.2",
    "ioredis": "^4.14.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-art-template": "^1.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "^3.0.0",
    "koa-helmet": "^5.2.0",
    "koa-jsonp": "^2.0.2",
    "koa-jwt": "^3.6.0",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.3",
    "koa-socket": "^4.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "koa2-request": "^1.0.4",
    "log4js": "^6.1.0",
    "mongoose": "^5.8.1",
    "multer": "^1.4.2",
    "mysql2": "^2.0.2",
    "nodemailer": "^6.4.2",
    "nodemailer-smtp-transport": "^2.7.4",
    "pure-svg-code": "^1.0.6",
    "qr-image": "^3.2.0",
    "supervisor": "^0.12.0",
    "svg-captcha": "^1.4.0"
  }
}
